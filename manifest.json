{
  "name": "Quick Maze",
  "description" : "A tiny maze game to play when bored.",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
      "default_popup": "index.html",
      "default_title": "Click (or Ctrl+Shift+F) to open maze",
      "default_icon": "mazeIcon.png"
    },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens index.html"
    }
  }
}
